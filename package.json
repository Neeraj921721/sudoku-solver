{
  "name": "sudoku-solver",
  "version": "1.0.0",
  "description": "This is Sudoku solver web project made using HTML,CSS,Javascript & Backtracking algorithm. Node.js Server is used in the back-end to run the project.",
  "main": "app.js",
  "scripts": {
    "start": "nodemon --watch public --ext html,css,js app.js",
    "dev": "nodemon app.js",
    "compile:sass": "node-sass --output-style compressed public/css/ -o public/css/",
    "build": "npm install && npm run compile:sass"
  },
  "author": "Neeraj Kumar Gond",
  "license": "ISC",
  "devDependencies": {
    "chokidar": "^3.6.0",
    "node-sass": "^9.0.0",
    "nodemon": "^3.1.0"
  },
  "dependencies": {
    "express": "^4.19.2"
  }
}
